<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>

	<servlet>
  		<servlet-name>RegistServlet</servlet-name>
  		<servlet-class>cn.hzu.web.servlet.RegistServlet</servlet-class>
    </servlet>
    <servlet>
  		<servlet-name>LoginServlet</servlet-name>
  		<servlet-class>cn.hzu.web.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
  		<servlet-name>MsgServlet</servlet-name>
  		<servlet-class>cn.hzu.web.servlet.MsgServlet</servlet-class>
    </servlet>
    <servlet>
  		<servlet-name>ElecListServlet</servlet-name>
  		<servlet-class>cn.hzu.web.servlet.ElecListServlet</servlet-class>
    </servlet>
     <servlet>
  		<servlet-name>ChangePwServlet</servlet-name>
  		<servlet-class>cn.hzu.web.servlet.ChangePwServlet</servlet-class>
    </servlet>
     <servlet>
  		<servlet-name>PayServlet</servlet-name>
  		<servlet-class>cn.hzu.web.servlet.PayServlet</servlet-class>
    </servlet>
  <servlet>
    <servlet-name>RootRegistServlet</servlet-name>
    <servlet-class>cn.hzu.web.servlet.RootRegistServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RootLoginServlet</servlet-name>
    <servlet-class>cn.hzu.web.servlet.RootLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StuListServlet</servlet-name>
    <servlet-class>cn.hzu.web.servlet.StuListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StuSaveServlet</servlet-name>
    <servlet-class>cn.hzu.web.servlet.StuSaveServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StuDelServlet</servlet-name>
    <servlet-class>cn.hzu.web.servlet.StuDelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FloorListServlet</servlet-name>
    <servlet-class>cn.hzu.web.servlet.FloorListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DormListServlet</servlet-name>
    <servlet-class>cn.hzu.web.servlet.DormListServlet</servlet-class>
  </servlet>



  
    <servlet-mapping>
  		<servlet-name>RegistServlet</servlet-name>
  		<url-pattern>/regist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
  		<servlet-name>LoginServlet</servlet-name>
  		<url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
  		<servlet-name>MsgServlet</servlet-name>
  		<url-pattern>/msg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
  		<servlet-name>ElecListServlet</servlet-name>
  		<url-pattern>/elecList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
  		<servlet-name>ChangePwServlet</servlet-name>
  		<url-pattern>/changePw</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
  		<servlet-name>PayServlet</servlet-name>
  		<url-pattern>/payservlet</url-pattern>
    </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RootRegistServlet</servlet-name>
    <url-pattern>/rootregist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RootLoginServlet</servlet-name> 
    <url-pattern>/rootlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StuListServlet</servlet-name>
    <url-pattern>/stuList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StuSaveServlet</servlet-name>
    <url-pattern>/stusave</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StuDelServlet</servlet-name>
    <url-pattern>/studel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FloorListServlet</servlet-name>
    <url-pattern>/floorList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DormListServlet</servlet-name>
    <url-pattern>/dormList</url-pattern>
  </servlet-mapping>
    
 
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
